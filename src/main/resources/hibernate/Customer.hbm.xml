<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.tcoveney.model.Customer" table="customers">
		<id name="id">
			<generator class="assigned"></generator>
		</id>
		<property name="name"></property>
		<property name="street"></property>
		<property name="city"></property>
		<property name="state"></property>
		<property name="zipcode"></property>
		<property name="homePhone" column="home_phone"></property>
		<property name="workPhone" column="work_phone"></property>
		<property name="email"></property>
		<property name="createdAt" type="timestamp" column="created_at"></property>
		<property name="updatedAt" type="timestamp" column="updated_at"></property>
	</class>
</hibernate-mapping>  